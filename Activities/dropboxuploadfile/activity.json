{
  "name": "Dropbox UploadFile",
  "version": "0.0.1",
  "type": "flogo:activity",
  "ref": "github.com/Sushil-Takkekar/Flogo_test/Activities/dropboxuploadfile",
  "description": "This activity is used to upload a file to the dropbox",
  "author": "Team LetsFlogo",
  "inputs":[
    {
      "name": "accessToken",
      "type": "string",
      "required": true
    },
    {
      "name": "sourceType",
      "type": "string",
      "allowed": [
        "File path",
        "Binary data"
      ],
      "value": "File path",
      "required": true
    },
    {
      "name": "dropboxDestPath",
      "type": "string",
      "required": true
    },
    {
      "name": "sourceFilePath",
      "type": "string"
    },
    {
      "name": "fileContent",
      "type": "string"
    }
  ],
  "outputs": [
    {
      "name": "result",
      "type": "string"
    }
  ]
}
